    "docker": {
        "diagnostics": {
            "deprecatedMaintainer": "error",
            "directiveCasing": "warning",
            "emptyContinuationLine": "warning",
            "instructionCasing": "warning",
            "instructionCmdMultiple": "warning",
            "instructionEntrypointMultiple": "warning",
            "instructionHealthcheckMultiple": "warning",
            "instructionJSONInSingleQuotes": "warning"
        },
        "languageserver": {
            "diagnostics": {
                "deprecatedMaintainer": "error",
                "directiveCasing": "warning",
                "emptyContinuationLine": "warning",
                "instructionCasing": "warning",
                "instructionCmdMultiple": "warning",
                "instructionEntrypointMultiple": "warning",
                "instructionHealthcheckMultiple": "warning",
                "instructionJSONInSingleQuotes": "warning"
            }
        }
    },
    "gopls": {
        "completionDocumentation": false,
        "usePlaceholders": true,
        "deepCompletion": true,
        "completeUnimported": true,
        "completionBudget": "100ms",
        "matcher": "fuzzy",
        "symbolMatcher": "fuzzy",
        "symbolStyle": "dynamic",
        "hoverKind": "fulldocumentation",
        "linksInHover": false,
        "importShortcut": "both",
        "analyses": {
            "fieldalignment": false,
            "unusedparams": false
        },
        "annotations": {
            "noEscape": false,
            "noNilcheck": false,
            "noInline": false,
            "noBounds": false
        },
        "codelenses": {
            "test": false,
            "generate": false,
            "tidy": false,
            "vendor": false,
            "add_dependency": false,
            "upgrade_dependency": false,
            "remove_dependency": false,
            "regenerate_cgo": false,
            "gc_details": false,
            "fill_struct": false,
            "undeclared_name": false,
            "extract_variable": false,
            "extract_function": false,
            "generate_gopls_mod": false
        },
        "staticcheck": false,
        "verboseOutput": false,
        "verboseWorkDoneProgress": false,
        "tempModfile": true,
        "gofumpt": false,
	"semanticTokens": true,
	"expandWorkspaceToModule": true,
	"experimentalWorkspaceModule": true,
	"experimentalDiagnosticsDelay": "0",
	"experimentalPackageCacheKey": true
    },
    "http": {
        "proxyStrictSSL": true
    },
    "json": {
        "format": {
            "enable": true
        },
        "schemas": [
            {
                "fileMatch": [
                    "swagger.json",
                    "**/swagger-spec/*.json"
